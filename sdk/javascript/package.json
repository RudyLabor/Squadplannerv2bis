{
  "name": "@squadplanner/sdk",
  "version": "1.0.0",
  "description": "Official Squad Planner JavaScript/TypeScript SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/squadplanner/sdk-js"
  },
  "keywords": [
    "squadplanner",
    "gaming",
    "squad",
    "sessions",
    "esports",
    "sdk"
  ],
  "author": "Squad Planner Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
